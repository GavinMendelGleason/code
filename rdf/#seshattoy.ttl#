	a owl:Class ;
	rdfs:comment "A variable that holds a definite data value object"@en ;
	rdfs:label "Definite Data Value"@en ;
	rdfs:subClassOf [ a owl:Restriction ;
			  rdfs:onProperty utv:definiteDataValue ;
			  owl:cardinality "1"^^xsd:nonNegativeInteger
			] .

