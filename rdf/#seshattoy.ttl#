	rdfs:comment "Assures that UnsignedLong value is a proper xsd:unsignedLong."@en ;
	rdfs:label "Unsigned Long Data Value Restriction"@en ;
	owl:allValuesFrom xsd:unsignedLong ;
	owl:onProperty utv:definiteDataValue .

