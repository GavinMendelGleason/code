utv:TemporalScopedDataVariable
	a owl:Class ;
	rdfs:comment "An uncertain data variable that also has temporal scoping restrictions."@en ;
	rdfs:label "Temporal Scoped Data Variable"@en ;
	rdfs:subClassOf utv:Variable ;
	owl:intersectionOf ( utv:UncertainDataVariable
			     utv:TemporalScoping
			   ) .

utv:TemporalScopedVariable
	a owl:Class ;
	rdfs:comment "An uncertain object variable that also has temporal scoping restrictions."@en ;
	rdfs:label "Temporal Scoped Variable"@en ;
	rdfs:subClassOf utv:Variable ;
	owl:intersectionOf ( utv:UncertainVariable
			     utv:TemporalScoping
			   ) .

utv:TemporalScoping
	a owl:Class ;
	rdfs:comment "Temporal constraints that can be defined for the span of a variable's  validity."@en ;
	rdfs:label "Temporal Scoping"@en ;
	owl:disjointUnionOf ( utv:Instant
			      utv:Interval
			      utv:UncertainInstant
			      utv:UncertainInterval
			    ) .

utv:UncertainBeginInterval
	a owl:Class ;
	rdfs:comment "A time interval that supports uncertainty for the beginning of the interval."@en ;
	rdfs:label "Uncertain Begin Interval"@en ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:cardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasEnd
			] ,
			[ a owl:Restriction ;
			  owl:minCardinality "2"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasBeginning
			] .

utv:UncertainBothInterval
	a owl:Class ;
	rdfs:comment "A time interval that supports uncertainty for the beginning and end of the interval."@en ;
	rdfs:label "Uncertain Both Interval"@en ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:minCardinality "2"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasEnd
			] ,
			[ a owl:Restriction ;
			  owl:minCardinality "2"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasBeginning
			] .

utv:UncertainDataVariable
	a owl:Class ;
	rdfs:comment "A data variable which is either definite (with our wwithout inference), has some uncertainty or is unknown."@en ;
	rdfs:subClassOf utv:Variable ;
	owl:disjointUnionOf ( utv:DatValue
			      utv:Unknown
			    ) .

utv:UncertainEndInterval
	a owl:Class ;
	rdfs:comment "A time interval that supports uncertainty for the end of the interval."@en ;
	rdfs:label "Uncertain End Interval"@en ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:minCardinality "2"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasEnd
			] ,
			[ a owl:Restriction ;
			  owl:cardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasBeginning
			] .

utv:UncertainInstant
	a owl:Class ;
	rdfs:comment "A temporal value that is true at a specific point in time but there are muliple possible values for the specific point in time."@en ;
	rdfs:label "Uncertain Instant"@en ;
	owl:subClassOf [ a owl:Restriction ;
			 owl:minCardinality "2"^^xsd:nonNegativeInteger ;
			 owl:onProperty utv:atDateTime
		       ] .

utv:UncertainInterval
	a owl:Class ;
	rdfs:comment "A time interval that supports uncertainty at one or both ends of the interval."@en ;
	rdfs:label "Uncertain Interval"@en ;
	owl:disjointUnionOf ( utv:UncertainEndInterval
			      utv:UncertainBeginInterval
			      utv:UncertainBothInterval
			    ) .

