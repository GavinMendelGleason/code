utv:Interval
	a owl:Class ;
	rdfs:comment "A temporal value that is true for an interval of time."@en ;
	rdfs:label "Interval"@en ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:cardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasEnd
			] ,
			[ a owl:Restriction ;
			  owl:cardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty utv:hasBeginning
			] .

utv:IntervalValue
	a owl:Class ;
	rdfs:comment "A collective class for both certain and uncertain intervals."@en ;
	rdfs:label "Interval Value"@en ;
	owl:disjointUnionOf ( utv:Interval
			      utv:UncertainInterval
			    ) .

