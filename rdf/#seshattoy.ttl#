	rdfs:range xsd:dateTime .

time:year
	a owl:DatatypeProperty ;
	rdfs:domain time:DateTimeDescription ;
	rdfs:range xsd:gYear .

time:years
	a owl:DatatypeProperty ;
	rdfs:domain time:DurationDescription ;
	rdfs:range xsd:decimal .

org:timezone
	a owl:Ontology ;
	rdfs:comment "\n      A time zone ontology.\n    "@en ;
	rdfs:seeAlso TR:owl-time ;
	owl:imports <http://www.daml.org/2001/09/countries/iso-3166-ont> ,
		    <http://www.daml.ri.cmu.edu/ont/USRegionState.daml> .

timezone:City
	a owl:Class ;
	rdfs:subClassOf timezone:PoliticalRegion .

timezone:Country
	a owl:Class ;
	rdfs:subClassOf timezone:PoliticalRegion .

timezone:County
	a owl:Class ;
	rdfs:subClassOf timezone:PoliticalRegion ;
	owl:subClassOf [ a owl:Restriction ;
			 owl:allValuesFrom timezone:State ;
			 owl:onProperty timezone:hasParentRegion
		       ] .

timezone:DLSendDate
	a owl:DatatypeProperty ;
	rdfs:range xsd:date .

timezone:DLSstartDate
	a owl:DatatypeProperty ;
	rdfs:range xsd:date .

timezone:DaylightSavingsPolicy
	a owl:Class .

timezone:EnumeratedDaylightSavingsPolicy
	a owl:Class ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:DLSstartDate
			] ,
			[ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:DLSendDate
			] ,
			timezone:DaylightSavingsPolicy .

timezone:GMToffset
	a owl:DatatypeProperty ;
	rdfs:domain timezone:TimeZone ;
	rdfs:range xsd:duration .

timezone:PoliticalRegion
	a owl:Class ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:name
			] ,
			[ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:hasParentRegion
			] ,
			[ a owl:Restriction ;
			  owl:maxCardinality "3"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:hasTimeZone
			] ,
			[ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:observesDaylightSavingsTime
			] ,
			[ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:hasDaylightSavingsPolicy
			] ,
			[ a owl:Restriction ;
			  owl:minCardinality "0"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:exceptionalRegion
			] ,
			[ a owl:Restriction ;
			  owl:minCardinality "0"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:timeZonePart
			] ,
			timezone:Region .

timezone:Region
	a owl:Class .

timezone:Reservation
	a owl:Class ;
	rdfs:subClassOf timezone:PoliticalRegion ;
	owl:subClassOf [ a owl:Restriction ;
			 owl:allValuesFrom timezone:Country ;
			 owl:onProperty timezone:hasParentRegion
		       ] .

timezone:State
	a owl:Class ;
	rdfs:subClassOf timezone:PoliticalRegion ;
	owl:subClassOf [ a owl:Restriction ;
			 owl:allValuesFrom timezone:Country ;
			 owl:onProperty timezone:hasParentRegion
		       ] .

timezone:TimeZone
	a owl:Class ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:name
			] ,
			[ a owl:Restriction ;
			  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
			  owl:onProperty timezone:GMToffset
			] .

timezone:countryOf
	a owl:ObjectProperty ;
	rdfs:range timezone:Country ;
	rdfs:subPropertyOf timezone:hasParentRegion .

timezone:exceptionalRegion
	a owl:ObjectProperty ;
	rdfs:domain timezone:Region ;
	rdfs:range timezone:Region .

timezone:hasAncestorRegion
	a owl:ObjectProperty ,
	  owl:TransitiveProperty ;
	rdfs:domain timezone:Region ;
	rdfs:range timezone:Region .

timezone:hasDaylightSavingsPolicy
	a owl:ObjectProperty ;
	rdfs:domain timezone:Region ;
	rdfs:range timezone:DaylightSavingsPolicy .

timezone:hasParentRegion
	a owl:ObjectProperty ;
	rdfs:subPropertyOf timezone:hasAncestorRegion .

timezone:hasTimeZone
	a owl:ObjectProperty ;
	rdfs:domain timezone:Region ;
	rdfs:range timezone:TimeZone .

timezone:name
	a owl:DatatypeProperty ;
	owl:equivalentProperty <http://www.daml.org/2001/09/countries/iso-3166-ont#name> .

timezone:observesDaylightSavingsTime
	a owl:DatatypeProperty ;
	rdfs:domain timezone:Region ;
	rdfs:range xsd:boolean .

timezone:stateOf
	a owl:ObjectProperty ;
	rdfs:range timezone:State ;
	rdfs:subPropertyOf timezone:hasParentRegion .

timezone:timeZonePart
	a owl:ObjectProperty ;
	rdfs:domain timezone:Region ;
	rdfs:range timezone:Region .
