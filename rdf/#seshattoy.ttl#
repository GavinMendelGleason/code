	rdfs:label "Unknown Value"@en ;
	rdfs:subClassOf utv:UnknownValueRestriction .

utv:UnknownValueRestriction
	a owl:Restriction ;
	rdfs:onProperty utv:unknownValue ;
	owl:hasValue true .

utv:UnsignedLongBagOfDataValuesRestriction
	a owl:Restriction ;
	rdfs:comment "Assures that UnsignedLong bag value has all elements of type xsd:unsignedLong."@en ;
	rdfs:label "Unsigned Long Bag of Data Values Restriction"@en ;
	owl:allValuesFrom xsd:unsignedLong ;
	owl:onProperty utv:dataValuesFrom .

