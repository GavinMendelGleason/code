utv:Inferred
	a owl:Class ;
	rdfs:comment "The value of the variable has been investigated but is inferred due to the limits of the investigators knowledge (Subjective Epistemic Vagueness )"@en ;
	rdfs:label "Inferred"@en ;
	rdfs:subClassOf utv:InferredRestriction .

utv:InferredDataValue
	a owl:Class ;
	rdfs:comment "A data variable value which was inferred"@en ;
	rdfs:label "Inferred Data Value"@en ;
	owl:intersectionOf ( utv:DefiniteDataValue
			     utv:Inferred
			   ) .

utv:InferredRestriction
	a owl:Restriction ;
	rdfs:onProperty utv:inferred ;
	owl:hasValue true .

utv:InferredValue
	a owl:Class ;
	rdfs:comment "An object variable value which was inferred"@en ;
	rdfs:label "Inferred Value"@en ;
	owl:intersectionOf ( utv:DefiniteValue
			     utv:Inferred
			   ) .

utv:Instant
	a owl:Class ;
	rdfs:comment "A temporal value that is true at a specific point in time."@en ;
	rdfs:label "Instant"@en ;
	owl:subClassOf [ a owl:Restriction ;
			 owl:cardinality "1"^^xsd:nonNegativeInteger ;
			 owl:onProperty utv:atDateTime
		       ] .

utv:InstantValue
	a owl:Class ;
	rdfs:comment "A temporal scoping value that is true at a certain or uncertain specific point in time."@en ;
	rdfs:label "Instant Value"@en ;
	owl:disjointUnionOf ( utv:Instant
			      utv:UncertainInstant
			    ) .

