    rdfs:comment "A variable holding a name of a seshat entity."
  ] ;
  seshat:TimeDaysVariable [
    a owl:Class ;
    rdfs:subClassOf dacura:UncertainTemporalFloatDataVariable ;
    rdfs:label "Time Days Variable", "Seshat variable for recoding a time in days as a measure of speed." ;
    dacura:unitOfMeasure seshat:day
  ] ;
