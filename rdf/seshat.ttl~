@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ns0: <http://localhost/all/ontology/seshat/> .

[]
  seshat: [
    a owl:Ontology ;
    dc11:title "Seshat Global History Databank Ontology" ;
    rdfs:comment "The Seshat Global History Databank ontology is designed to describe human societies throughout time." ;
    rdfs:label "Seshat Global History Databank Ontology" ;
    owl:versionInfo "0.1.16" ;
    dc11:date "2015-11-10" ;
    dc11:creator "Rob Brennan", "Kevin Feeney", "Gavin Mendel-Gleason" ;
    dc11:contributor "Odhran Gavin", "Bojan Bozic", "Pieter Francois", "Harvey Whitehouse", "Arkadiusz Marciniak", "Thomas E. Currie", "Christopher Chase-Dunn", "Jack A. Goldstone", "J. G. Manning", "Peter N. Peregrine", "Frances Reynolds", "Peter J. Richerson", "Enrico Spolaore", "Romain Wacziarg", "Douglas R. White", "David S. Wilson", "Andrey Korotayev", "David Baker", "Johannes Preiser-Kapeller", "Nikolay Kradin", "Daniel Hoyer", "Peter Turchin" ;
    dc11:rights "This ontology is distributed under a Creative Commons Attribution License - http://creativecommons.org/licenses/by/3.0" ;
    dc11:description "This ontology will be used by the Evolution Institute and its partners to describe time series data collected about all human societies."
  ] ;
  seshat:ImageVariable [
    a owl:Class ;
    rdfs:subClassOf dacura:UncertainTemporalAnyURIDataVariable ;
    rdfs:label "Image" ;
    rdfs:comment "A reference to an image file."
  ] ;
  seshat:LinearDimension [
    a owl:Class ;
    rdfs:subClassOf dacura:UncertainTemporalNonNegativeIntegerDataVariable ;
    rdfs:label "Linear dimension" ;
    rdfs:comment "A datatype object used to specify a height, length, width or other linear distance measured in metres."
  ] ;
  seshat:HumanEffort [
    a owl:Class ;
    rdfs:subClassOf dacura:UncertainTemporalNonNegativeIntegerDataVariable ;
    rdfs:label "Human Effort" ;
    rdfs:comment "A datatype object used to specify a number of years of human effort, for example to express a cost."
  ] ;
  seshat:NameVariable [
    a owl:Class ;
    rdfs:subClassOf dacura:UncertainTemporalStringDataVariable ;
